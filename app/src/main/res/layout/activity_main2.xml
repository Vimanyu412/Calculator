<?xml version="1.0" encoding="utf-8"?>
<android.widget.RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="vimanyu.calculator.Main2Activity">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:height="50sp"
        android:width="350sp"
        android:text="@string/value_is"
        android:textSize="40sp"
        android:textAlignment="center"
        android:layout_above="@+id/textVeiw33"
        android:layout_alignLeft="@+id/textVeiw33"
        android:layout_alignStart="@+id/textVeiw33"
        android:layout_marginBottom="67dp" />

    <TextView
        android:id="@+id/textVeiw33"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:width="350sp"
        android:height="50sp"
        android:text="@string/textview12"
        android:textAlignment="center"
        android:textSize="40sp" />

    <Button
        android:id="@+id/button13"
        android:onClick="return123"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textVeiw33"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="83dp"
        android:text="@string/button1234" />

    <Button
        android:onClick="prevCalc"
        android:id="@+id/button14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:text="@string/button12344"
        android:layout_below="@+id/button13"
        android:layout_centerHorizontal="true" />
</android.widget.RelativeLayout>
